***********************************************************************
* Set supply and library
***********************************************************************
.INCLUDE ../mos_model3
.options temp=27
.options post

***********************************************************************
* Define power supply
***********************************************************************
* Vdd     Vdd      0       2.5

***********************************************************************
* Define Subcircuits
***********************************************************************
* MOSFETs General form: MXXXX ND NG NS NB MNAME <L=VAL> <W=VAL>

***********************************************************************
* Stimulus
***********************************************************************
* Format of pulse input:
* PULSE(v_initial v_final t_delay t_rise t_fall t_pulsewidth t_period)
Va0     V00     0       PULSE(0   1  0ns 0.1ns 0.1ns 120ns 122ns)
Va1     V10     0       PULSE(0   1  0ns 0.1ns 0.1ns 120ns 122ns)
Vb      V20     0       PULSE(0   1  0ns 0.1ns 0.1ns 120ns 122ns)
Vc      V30     0       PULSE(0   1  0ns 0.1ns 0.1ns 120ns 122ns)

***********************************************************************
* Top level simulation netlist
***********************************************************************
* a for b
C01      V01        0     300p
R01      V00      V01     100     

* a for c
C11      V11        0     600p
R11      V10      V11     100     

* b
C21      V21        0     100p
C22      V22        0     100p
R21      V20      V21     100
R22      V21      V22     100

* c
C31      V31        0     100p
C32      V32        0     100p
C33      V33        0     100p
R31      V30      V31     100
R32      V31      V32     100
R33      V32      V33     100

.tran .001ns 120ns
* .plot TRAN V(XA) V(XB) V(XC) V(XD) V(F)

***********************************************************************
* End of Deck
***********************************************************************
.end
