* ことえり
記号を常に半角に
http://www9.atwiki.jp/defaults/pages/21.html
# space
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add " " -bool no
# back-quote
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "\`" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "@" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "#" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "$" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "%" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "^" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "*" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "=" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "|" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add ";" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add ":" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "'" -bool no
# double-quote
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "'\"'" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "'>'" -bool no
$ defaults write com.apple.inputmethod.Kotoeri zhsy -dict-add "'<'" -bool no
$ killall Kotoeri

