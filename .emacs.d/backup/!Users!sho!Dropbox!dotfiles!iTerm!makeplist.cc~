#include <iostream>
#include <stdio.h>

using namespace std;

int main(int argc, char *argv[])
{
  for (char c=33; c<64; c++) {
  // int chars[] = {'!',''}
  // char c = '.';
  printf("<key>0x%02x-0x40000</key>\n", c);
  printf("<dict>\n");
  printf("  <key>Action</key>\n");
  printf("  <integer>10</integer>\n"); // send escape sequences
  printf("  <key>Text</key>\n");
  printf("  <string>[27;5;%02d</string>\n", c);
  printf("</dict>\n");
  }  
  return 0;
}


    // <key>xterm Defaults</key>
    // <dict>

    //   <key>0x2e-0x40000</key>
    //   <dict>
	// <key>Action</key>
	// <integer>10</integer>
	// <key>Text</key>
	// <string>[27;5;46</string>
    //   </dict>
